{
  "name": "webpack-academy",
  "version": "2.0.0",
  "description": "basic webpack configuration example",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/url-here",
    "type": "git"
  },
  "author": "Lukas Pospisil",
  "license": "MIT",
  "scripts": {
    "start": "yarn webpack-dev-server --env.env dev",
    "build": "yarn webpack --env.env prod",
    "prebuild": "rimraf dist && yarn format && yarn lint",
    "cypress": "cypress open",
    "cypress:all": "cypress run",
    "test": "jest --config src/jest.config.js",
    "test:watch": "yarn test  --watch",
    "precommit": "lint-staged",
    "format": "yarn prettier --write",
    "lint": "eslint --ext .js src --fix",
    "prettier": "prettier *.js './**/*.{js,json}'",
    "debug": "node --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "webpack-dev-server --port 3030 --hot",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.js": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@types/enzyme": "^3.1.12",
    "@types/jasmine": "^2.8.8",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/sinon": "^5.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "cypress": "^3.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "jest-styled-components": "^6.1.0",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "open-browser-webpack-plugin": "^0.0.5",
    "prettier": "^1.14.0",
    "react-test-renderer": "^16.4.2",
    "regenerator-runtime": "^0.12.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.4",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.3.3",
    "styled-normalize": "^8.0.0"
  }
}
